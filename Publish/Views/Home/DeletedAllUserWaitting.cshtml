
@{
    ViewBag.Title = "Confirmation du suppresion des utilisateurs";
}


<div class="flex-content flex-jcenter-acenter flex-col pop-up">
    <div class="popup-container with-shadow">
        <div class="head gradient-blue">
            <h1>Terminé</h1>
        </div>
        <div class="content-section text-centered icon-bottom">
            <i class="pop-icon green-bg"></i>
            <p>Voulez-vous vraiment supprimer la sélection ?</p>
            <input type="submit" value="Supprimer" class="btn btn-primary" onclick="@("window.location.href='" + @Url.Action("DeleteAllUserSelected", "Home") + "' ");">
            <input type="submit" value="Non" class="btn btn-warning" onclick="@("window.location.href='" + @Url.RouteUrl("Home", new {controller = "Home", action = "ListeUser" }) + "' ");">


        </div>
    </div>

</div>


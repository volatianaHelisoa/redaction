<script type="text/javascript">

    jQuery(document).ready(function () {


        var mot_cle_pricipal = $("#mot_cle_pricipal").data("lastValue");
        var d = new Date();
        var month = d.getMonth() + 1;
        var filename = "Guide-" + mot_cle_pricipal + "-" + month + ".pdf";
        console.log(filename);

        var html = $('#wrapper-guide').html(); // document.body.innerHTML;
        $.ajax({
            url: "/Guide/DownloadPDFIText",

            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                htmlContent: html,
                mot_cle_pricipal: mot_cle_pricipal,
            }),
            dataType: "json",
            //processdata: false,
            success: function (response) {
                if (response.Valid) {
                   
                    var url = "http://" + $(location).attr('host') + "/Pdf/Guides/" + filename;

                    $(".fileDownloadPdf").attr("href", url);
                    console.log($(".fileDownloadPdf").prop('href'));
                    //$(".fileDownloadPdf").trigger("click");
                }

                else {
                    console.log('download failed.');
                    //alert(response.Errors);
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                console.log("XMLHttpRequest=" + XMLHttpRequest.responseText + "\ntextStatus=" + textStatus + "\nerrorThrown=" + errorThrown);
            }
        });

        $(".fileDownloadPdf").on("click", function (e) {
            //$.fileDownload($(this).prop('href'), {
            //    preparingMessageHtml: "We are preparing your report, please wait...",
            //    failMessageHtml: "There was a problem generating your report, please try again."
            //});
            //return false;
        });
    });
</script>



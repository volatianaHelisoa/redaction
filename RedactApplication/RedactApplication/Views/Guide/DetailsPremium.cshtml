<body>
    <div id="wrapper-guide">
        <div id="main">
            <div id="fond">

                <div id="query">@Model.mot_cle_pricipal</div>

            </div>
            <div class="grammes">
                <div class="toptitle">TOP TERMES</div>
                <div class="grams grams1">
                    <div class="label">&nbsp;</div>
                    @{ string[] keysArray = Model.grammes1.Split(','); }
                    <ul class="col col1">
                        @foreach (var grammes1 in keysArray)
                        {
                            <li>@grammes1</li>
                        }
                    </ul>

                    <div class="clearfix"></div>
                </div>
            </div>
            <div id="footer">
                <div class="clearfix"></div>
            </div>
        </div>
        <div id="main" class="page2">

            <div class="grammes">
                <div class="grams grams2">
                    <div class="label">&nbsp;</div>
                    @{ string[] grammes2Array = Model.grammes2.Split(','); }
                    <ul class="col col1">
                        @foreach (var grammes2 in grammes2Array)
                        {
                            <li>@grammes2</li>
                        }
                    </ul>

                    <div class="clearfix"></div>
                </div>
            </div>


            <div class="grammes">
                <div class="grams grams3">
                    <div class="label">&nbsp;</div>
                    @{ string[] grammes3Array = Model.grammes3.Split(','); }
                    <ul class="col col1">
                        @foreach (var grammes3 in grammes3Array)
                        {
                            <li>@grammes3</li>
                        }
                    </ul>

                    <div class="clearfix"></div>
                </div>
            </div>


            <div class="grammes">
                <div class="toptitle">ENTITÉS NOMMÉES</div>
                <div class="grams grams1">
                    @{ string[] entitiesArray = Model.entities.Split(','); }
                    <ul class="col col1">
                        @foreach (var entities in entitiesArray)
                        {
                            <li>@entities</li>
                        }
                    </ul>

                    <div class="clearfix"></div>
                </div>
            </div>

            <div id="footer">

                <div class="clearfix"></div>
            </div>
        </div>

    </div>   
        <div id="toast" class="with-shadow">
            <a class="fileDownloadPdf btn btn-primary" href="@Model.lien_pdf" target="_blank"><i class="fa fa-download"></i> Télécharger</a>
            <input hidden id="mot_cle_pricipal" data-last-value="@Model.mot_cle_pricipal" />
            @Html.ActionLink("Retour", "ListGuide", new { controller = "Guide" }, new { @class = "btn btn-warning" })
        </div>

        <script type="text/javascript" src="http://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>

@{
    ViewBag.Title = "Confirmer création commande";

}



<div class="flex-content flex-jcenter-acenter flex-col pop-up">
    <div class="popup-container with-shadow">
        <div class="head gradient-blue">
            <h1>Terminé</h1>
        </div>
        <div class="content-section text-centered icon-bottom">
            <i class="pop-icon green-bg"></i>
            <p>La commande a bien été créee !</p>

            @using (Html.BeginForm("ListGuide", "Guide", FormMethod.Post))
            {
                <input type="submit" value="Ok" class="btn btn-primary" id="btnEnregistrerCmde">

                @Html.ActionLink("Créer", "Create", "Guide", new { }, new { @class = "btn btn-warning" })
            }
        </div>
    </div>

</div>
